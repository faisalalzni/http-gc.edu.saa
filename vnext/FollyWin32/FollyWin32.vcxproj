      <Platform>Win32</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <ItemGroup>
    <None Include="README.md" />
  </ItemGroup>
  <ItemGroup>
    <ClCompile Include="$(FollyDir)\folly\portability\SysMman.cpp" />
  </ItemGroup>
  <ItemGroup>
    <ClInclude Include="$(FollyDir)\folly\portability\SysMman.h" />
  </ItemGroup>
